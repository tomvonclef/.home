alias ls="ls --color=auto"
alias ll="ls -l"
alias la="ls -A"
alias lll="ls -lA --color=always | less -r"
alias lsd="ll | grep ^d"

alias gs="git status"
alias ga="git add"
alias gc="git commit -m "
alias gp="git push"

alias egrep='egrep --color=auto'
alias fgrep='fgrep --color=auto'
alias grep='grep --color=auto'

